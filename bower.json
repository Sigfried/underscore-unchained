{
  "name": "underscore-unchained",
  "version": "0.1.1",
  "homepage": "https://github.com/Sigfried/underscore-unchained",
  "authors": [
    "(Sigfried Gold <sigfried@sigfried.org>)"
  ],
  "description": "Enhance arrays and other objects with Underscore methods so you don't need to mess with _.chain() and .value() every time you do two Underscore methods in sequence.",
  "dependencies": {
    "1670507": "https://gist.github.com/1670507.git",
    "underscore": "1.4.4"
  },
  "keywords": [
    "underscore",
    "syntactic",
    "sugar",
    "reckless",
    "prometheus",
    "titans",
    "olympus",
    "shelley",
    "convenience",
    "chain",
    "unchain"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
